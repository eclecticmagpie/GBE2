MasterEvent

    def _SetStartTime(self, DateTime):
        StartTime = DateTime

    def _SetStopTime(self, DateTime):
        StopTime = DateTime

    def _SetBlocking(self, block):
        Blocking = Blocking

    def _SetTypeList(self, Types):
        TypeList = types

    def _AddTypeList(self, Type):
        TypeList.append(Type)

    def _GetStartTime(self):
        return self.StartTime

    def _GetStopTime(self):
        return self.StopTime

    def _GetBlocking(self):
        return self.Blocking

    def _GetTypeList(self):
        return self.TypeList


